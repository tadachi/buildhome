{
  "name": "buildhome",
  "description": "A web server for my homepage and projects.",
  "main": "server.js",
  "private": true,
  "scripts": {
    "build": "babel server/src --out-dir ./",
    "dev": "nodemon --watch server/src --exec npm run build",
    "start": "nodemon server.js"
  },
  "dependencies": {
    "async": "^0.9.0",
    "events": "^1.0.2",
    "express": "^4.12.3",
    "serve-favicon": "^2.2.0",
    "socket.io": "^1.3.5",
    "vhost": "^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "debug": "^2.2.0",
    "nodemon": "^1.9.2"
  }
}
